{
  "name": "guocha",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/app.js",
    "dev": "umi dev",
    "dev:local": "COURSE_PORTAL_API_URL=http://localhost:5103 umi dev",
    "dev:prod": "DEV_PROD=ture umi dev",
    "build": "UMI_ENV=prod umi build",
    "test": "umi test",
    "test-build": "UMI_ENV=prod umi build",
    "precommit": "lint-staged",
    "eslint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "eslint:ts": "eslint src --ext .ts,.tsx",
    "eslint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "check-version": "umi -v --verbose",
    "check-code": "tsc --noEmit && lint-staged"
  },
  "dependencies": {
    "@rematch/core": "^1.1.0",
    "@rematch/loading": "^1.1.3",
    "antd": "^4.0.4",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.3.1",
    "react-use": "^13.27.1",
    "styled-components": "^4.3.2",
    "umi": "3.0.14"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.1.19",
    "@umijs/plugin-qiankun": "2.0.5",
    "@umijs/preset-react": "^1.4.8",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^4.2.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
